-- | Exercises on Boolean Relations.
module Koans.Boolean

logicalAND : Bool
logicalAND = True && __ == True

logicalOR : Bool
logicalOR = False || __ == False

logicalNOT : Bool
logicalNOT = not __ == False

logicalPredicate : Bool
logicalPredicate = not ( __ && True) || __ && False == True

