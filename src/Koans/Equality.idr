-- | Equality Operations
module Koans.Equality

equalityEq : Bool
equalityEq = __ == true

equalityNeq : Bool
equalityNeq = __ == 3

equalityGeq : Bool
equalityGeq = __ >= 4

equalityGtr : Bool
equalityGtr = __ > 4

equalityLeq : Bool
equalityLeq = __ <= 5

equalityLess : Bool
equalityLess = __ < 6
